<!-- Begin Page Content -->
<div class="container-fluid">

    <!-- Page Heading -->
    <h1 class="h3 mb-1 text-gray-800">Meta SEM</h1>
    <p class="mb-4">If the r package or rserve is not installed, please click
        <a href="https://getbootstrap.com/docs">this link</a> to install it before running the analysis.</p>

    <!-- Content Row -->
